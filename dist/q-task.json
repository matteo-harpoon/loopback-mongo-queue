{
  "name": "QTask",
  "base": "PersistedModel",
  "strict": false,
  "mongodb": {
    "allowExtendedOperators": true
  },
  "properties": {
    "chain": {
      "type": [
        "string"
      ]
    },
    "events": {
      "type": [
        "object"
      ],
      "default": []
    },
    "status": {
      "type": "string",
      "default": "qQueued"
    },
    "params": {
      "type": "object"
    },
    "qQueue": {
      "type": "string",
      "default": "default"
    },
    "count": {
      "type": "number",
      "default": 0
    },
    "attempts": {
      "type": "number",
      "default": 5
    },
    "remaining": {
      "type": "number",
      "default": 5
    },
    "delay": {
      "type": "date",
      "defaultFn": "now"
    },
    "priority": {
      "type": "number",
      "default": 0
    },
    "ended": {
      "type": "date"
    },
    "enqQueued": {
      "type": "date",
      "defaultFn": "now"
    },
    "result": {
      "type": "object"
    },
    "error": {
      "type": "object"
    },
    "stack": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {
    "item": {
      "type": "belongsTo",
      "polymorphic": true
    }
  },
  "acls": [
    {
      "principalType": "Role",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "Role",
      "principalId": "qWorkflow:read",
      "permission": "ALLOW",
      "accessType": "READ"
    }
  ],
  "methods": {}
}
